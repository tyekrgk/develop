d=a.data("stockLastInfo").originData,h=d[0][2],d=d[d.length-1][2],c=new Date,e=""+c.getHours(),c=""+(c.getMinutes()-1),e=e+c,a=a.siblings(".data-line"),c=a.find("span"),f=b.price.toFixed(2),b=b.pre,g=(f-b).toFixed(2),i=(100*(g/b)).toFixed(2)+"%";c.removeClass("hqred").removeClass("hqgren").removeClass("equal").addClass(0<f-b?"red":0>f-b?"green":"equal");c.eq(0).html(f);c.eq(1).html(i);c.eq(2).html(g);a.find("p").html("行情时间："+d.substr(0,2)+":"+d.substr(2,2));e>=h&&e<=d?a.find("strong").attr("class",
